    <!--Experiment Trials -->
    <!-- 8 training trials -->
    <trial>
        <name>Training</name>
        <reps>1</reps>
        <groups>
            <group>ControlNoObs</group>
            <group>ControlSmall</group>
            <group>ControlLarge</group>
            <group>VentralNoObs</group>
            <group>VentralSmall</group>
            <group>VentralLarge</group>
            <group>DorsalNoObs</group>
            <group>DorsalSmall</group>
            <group>DorsalLarge</group>
            <group>ControlNegNoObs</group>
            <group>ControlNegSmall</group>
            <group>ControlNegLarge</group>
        </groups>
        <initialTasks>
            <task>
                <name>PlaceRobotInitiallyTask</name>
                <params>
                    <initialPosition>S</initialPosition>
                </params>
            </task>
            <!-- <task> <name>ActivateFeeders</name> <params> </params> </task> -->
            <task>
                <name>FlashFeeder</name>
            </task>
        </initialTasks>
        <afterCycleTasks>
            <task>
                <name>FlashFeeder</name>
            </task>
            <task>
                <name>GiveFood</name>
            </task>
            <task>
				<name>ResetRobotAte</name>
			</task>
            <!-- <task> <name>UnflashFeeder</name> </task> -->
        </afterCycleTasks>
        <afterTrialTasks>
