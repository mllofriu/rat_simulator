<?xml version="1.0" encoding="us-ascii"?>
<!-- Just a test of new format -->

<simulation>
    <!--Robot startin points-->
    <point name='W' xp='-0.4' yp='0' zp='0' rot='0'/>
    <point name='N' xp='0' yp='0' zp='-0.4' rot='0'/>
    <point name='E' xp='0.4' yp='0' zp='0' rot='0'/>
    <point name='S' xp='0' yp='0' zp='0.35' rot='0'/>

    <!--Hidden platform points-->
    <point name='SE' xp='0.2' yp='0' zp='0.2' rot='0'/>
    <point name='SW' xp='-0.2' yp='0' zp='0.2' rot='0'/>
    <point name='NE' xp='0.2' yp='0' zp='-0.2' rot='0'/>
    <point name='NW' xp='-0.2' yp='0' zp='-0.2' rot='0'/>
    
    <!--Subjects-->
    <subject name='rat1'/>
    <subject name='rat2'/>
    <subject name='rat3'/>
    <subject name='rat4'/>
    <subject name='rat5'/>
    <subject name='rat6'/>
    <subject name='rat7'/>
    <subject name='rat8'/>
    
    <!--Subject groups-->
    <group name='g1'>
        <member name='rat1'/>
        <member name='rat2'/>
        <member name='rat3'/>
        <member name='rat4'/>
        <member name='rat5'/>
        <member name='rat6'/>
        <member name='rat7'/>
        <member name='rat8'/>
    </group>

    <!--Experiment Trials-->
    <trial>
        <type>training</type>
        <name>train</name>
        <start>S</start>
        <time>2000</time>
        <reps>16</reps>
        <maze>/mazes/morrisCircular.xml</maze>
        <subjectGroup>g1</subjectGroup>
    </trial>
    <!-- 4 testing trials-->
    <trial>
        <type>testing</type>
        <name>test</name>
        <start>S</start>
        <time>500</time>
        <reps>4</reps>
        <subjectGroup>g1</subjectGroup>
        <maze>/mazes/morrisCircular.xml</maze>
    </trial>
    


</simulation>