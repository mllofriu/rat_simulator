<?xml version="1.0" encoding="us-ascii"?>
<!-- Just a test of new format -->

<simulation>
    <!--Robot startin points-->
    <point name='W' xp='-0.4' yp='0' zp='0' rot='0'/>
    <point name='N' xp='0' yp='0' zp='-0.4' rot='0'/>
    <point name='E' xp='0.4' yp='0' zp='0' rot='0'/>
    <point name='S' xp='0' yp='0' zp='0.35' rot='0'/>

    <!--Hidden platform points-->
    <point name='SE' xp='0.2' yp='0' zp='0.2' rot='0'/>
    <point name='SW' xp='-0.2' yp='0' zp='0.2' rot='0'/>
    <point name='NE' xp='0.2' yp='0' zp='-0.2' rot='0'/>
    <point name='NW' xp='-0.2' yp='0' zp='-0.2' rot='0'/>
    
    <!--Subjects-->
    <subject name='rat1'/>
    <subject name='rat2'/>
    <subject name='rat3'/>
    <subject name='rat4'/>
    <subject name='rat5'/>
    <subject name='rat6'/>
    <subject name='rat7'/>
    <subject name='rat8'/>
    <subject name='rat9'/>
    <subject name='rat10'/>
    <subject name='rat11'/>
    <subject name='rat12'/>
    <subject name='rat13'/>
    <subject name='rat14'/>
    <subject name='rat15'/>
    <subject name='rat16'/>
    

    <!--Subject groups-->
    <group name='g1'>
        <member name='rat1'/>
        <member name='rat2'/>
        <member name='rat3'/>
        <member name='rat4'/>
        <member name='rat5'/>
        <member name='rat6'/>
        <member name='rat7'/>
        <member name='rat8'/>
    </group>
    <group name='g2'>
        <member name='rat9'/>
        <member name='rat10'/>
        <member name='rat11'/>
        <member name='rat12'/>
        <member name='rat13'/>
        <member name='rat14'/>
        <member name='rat15'/>
        <member name='rat16'/>
    </group>
    <!--Experiment Trials-->
    <trial>
        <type>training</type>
        <name>train</name>
        <start>S</start>
        <time>2000</time>
        <reps>32</reps>
        <maze>/edu/usf/ratsim/experiment/mazes/morrisCircular.xml</maze>
	<groups>
		<group>g1</group>
		<group>g2</group>
	</groups>

    </trial>
    <!-- 4 testing trials-->
    <trial>
        <type>testing</type>
        <name>test</name>
        <start>S</start>
        <time>500</time>
        <reps>8</reps>
        <groups>
		<group>g1</group>
		<group>g2</group>
	</groups>
        <maze>/edu/usf/ratsim/experiment/mazes/morrisCircular.xml</maze>
    </trial>
    


</simulation>
