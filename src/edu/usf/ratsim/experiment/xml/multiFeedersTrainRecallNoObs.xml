				</task>
			</afterCycleTasks>
			<afterCycleLoggers>
				<logger>
					<name>edu.usf.experiment.log.SubjectPositionLogger</name>
					<params>
					</params>
				</logger>
			</afterCycleLoggers>
			
			<afterEpisodeLoggers>
				<logger>
					<name>edu.usf.experiment.log.WallLogger</name>
					<params>
					</params>
				</logger>
				<logger>
					<name>edu.usf.experiment.log.ValueLogger</name>
					<params>
						<circle>false</circle>
						<interval>.1</interval>
						<angleInterval>.78</angleInterval>
						<numIntentions>1</numIntentions>
					</params>
				</logger>
			</afterEpisodeLoggers>
			<afterEpisodeTasks>
				
			</afterEpisodeTasks>
			<stopConditions>
				<condition>
					<name>edu.usf.experiment.condition.MaxCycles</name>
