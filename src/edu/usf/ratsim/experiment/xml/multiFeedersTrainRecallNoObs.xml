			<afterCycleTasks>
				<task>
					<name>edu.usf.experiment.task.SwitchFeeder</name>
				</task>
				<task>
					<name>edu.usf.experiment.task.ReleaseFoodFromFeeders</name>
				</task>
				<task>
					<name>edu.usf.experiment.task.TimeoutFlashFeeder</name>
					<params>
						<timeout>200</timeout>
					</params>
				</task>
				<task>
