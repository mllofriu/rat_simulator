<?xml version="1.0" encoding="us-ascii"?>
	
<experiment>
	<seed>32423</seed>
	
	<universe>
		<name>edu.usf.ratsim.experiment.universe.virtual.VirtUniverse</name>
		<params>
			<closeToFoodThrs>0.15</closeToFoodThrs>
			<maze>src/edu/usf/ratsim/experiment/mazes/multiFeeders.xml</maze>
			<display>false</display>
		</params>
	</universe>
	
	<robot>
		<name>edu.usf.ratsim.robot.virtual.VirtualRobot</name>
		<params>
			<noise>.2</noise>
			<translationRotationNoise>.195</translationRotationNoise>
			<lookaheadSteps>5</lookaheadSteps>
			<halfFieldOfView>1.83</halfFieldOfView>
			<visionDist>10</visionDist>
			<closeThrs>.1</closeThrs>
		</params>
	</robot>
	
	<model>
		<name>edu.usf.ratsim.experiment.subject.MultiScaleArtificialPCSubject</name>
		<params>
			<step>.05</step>
			<leftAngle>0.37</leftAngle>
			<rightAngle>-0.37</rightAngle>
			<numPCLayers>2</numPCLayers>
			<numPCCellsPerLayer>500</numPCCellsPerLayer>
			<minPCRadius>.05</minPCRadius>
			<maxPCRadius>.3</maxPCRadius>
			<placeCells>wallGoalExponential</placeCells>
			<goalCellProportion>0.5</goalCellProportion>
			<numHDLayers>1</numHDLayers>
			<numHDCellsPerLayer>256</numHDCellsPerLayer>
			<minHDRadius>.314</minHDRadius>
			<maxHDRadius>6.28</maxHDRadius>
			<numIntentions>8</numIntentions>
			<deterministicActionSelection>true</deterministicActionSelection>
			<foodReward>10000</foodReward>
			<nonFoodReward>0</nonFoodReward>
			<discountFactor>.95</discountFactor>
			<alpha>.9</alpha>
			<initialValue>0</initialValue>
			<flashingReward>5000</flashingReward>
			<nonFlashingReward>1000</nonFlashingReward>
			<estimateValue>true</estimateValue>
			<cellContribution>1</cellContribution>
			<explorationReward>0</explorationReward>
			<attentionExploringVal>100</attentionExploringVal>
			<maxAttentionSpan>20</maxAttentionSpan>
			<explorationHalfLifeVal>.125</explorationHalfLifeVal>
			<rlType>actorCritic</rlType>
			<voteType>halfAndHalfConnection</voteType>
			<xmin>-.5</xmin>
			<ymin>-.5</ymin>
			<xmax>.5</xmax>
			<ymax>.5</ymax>
			<maxActionsSinceForward>20</maxActionsSinceForward>
			<stillExplorationVal>10000</stillExplorationVal>
		</params>
	</model>
	
	
	<group>
		<name>Control</name>
		<numMembers>16</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	<group>
		<name>Dorsal</name>
		<numMembers>16</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	<group>
		<name>Ventral</name>
		<numMembers>16</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	<group>
		<name>NegControl</name>
		<numMembers>16</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	
	
	<group>
		<name>TimeoutControl</name>
		<numMembers>16</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	<beforeExperimentTasks>
		<!--<task> <name>edu.usf.experiment.task.EnableRandomFeedersTriangle</name> <params>
			</params> </task>-->
		<task>
			<name>edu.usf.experiment.task.EnableFeeders</name>
			<params>
				<feeders>0,2,5</feeders>
			</params>
		</task>
	</beforeExperimentTasks>
	
	<beforeExperimentLoggers>
		<logger>
			<name>edu.usf.experiment.log.WallLogger</name>
			<params>
			</params>
		</logger>
		<logger>
			<name>edu.usf.ratsim.experiment.log.PCLogger</name>
		</logger>
	</beforeExperimentLoggers>
	<beforeExperimentPlotters>
		<plotter>
			<name>edu.usf.experiment.plot.CSVConverter</name>
		</plotter>
		<!--<plotter>
			<name>edu.usf.ratsim.experiment.plot.PCPlotter</name>
		</plotter>-->
	</beforeExperimentPlotters>
	
	<trial>
		<name>Training</name>
		
		<groups>
			<group>Control</group>
			<group>Dorsal</group>
			<group>Ventral</group>
			<group>NegControl</group>
		</groups>
		<beforeTrialTasks>
		</beforeTrialTasks>
		<episodes>
			<number>1</number>
			<sleep>00</sleep>
			<beforeEpisodeTasks>
				<task>
					<name>edu.usf.experiment.task.ActivateAllEnabledFeeders</name>
					<params>
					</params>
				</task>
				<task>
					<name>edu.usf.experiment.task.FlashActivateRandomFeeder</name>
					<params>
					</params>
				</task>
				<task>
					<name>edu.usf.experiment.task.PlaceRobotInitally</name>
					<params>
						<point>
							<x>0</x>
							<y>0</y>
							<theta>3.14</theta>
						</point>
					</params>
				</task>
			</beforeEpisodeTasks>
			<beforeCycleTasks>
			</beforeCycleTasks>
			<afterCycleTasks>
				<task>
					<name>edu.usf.experiment.task.SwitchFlashingFeeder</name>
				</task>
				<task>
					<name>edu.usf.experiment.task.ReleaseFoodFromFeeders</name>
				</task>
			</afterCycleTasks>
			<afterCycleLoggers>
				<logger>
					<name>edu.usf.experiment.log.SubjectPositionLogger</name>
					<params>
					</params>
				</logger>
				
				
			</afterCycleLoggers>
			
			<afterEpisodeLoggers>
				<logger>
					<name>edu.usf.experiment.log.WallLogger</name>
					<params>
					</params>
				</logger>
				<logger>
					
					<name>edu.usf.experiment.log.ValueLogger</name>
					<params>
						<circle>false</circle>
						<interval>.1</interval>
						<angleInterval>.78</angleInterval>
						<numIntentions>1</numIntentions>
					</params>
				</logger>
				
			</afterEpisodeLoggers>
			<afterEpisodeTasks>
				
			</afterEpisodeTasks>
			<stopConditions>
				<condition>
					<name>edu.usf.experiment.condition.MaxCycles</name>
					<params>
						<max>50000</max>
					</params>
				</condition>
				<condition>
					<name>edu.usf.experiment.condition.FoundNFoodStopCond</name>
					<params>
						<n>100</n>
					</params>
				</condition>
			</stopConditions>
			<afterEpisodePlotters>
				<plotter>
					<name>edu.usf.experiment.plot.CopyMazeFile</name>
				</plotter>
				<plotter>
					<name>edu.usf.experiment.plot.CSVConverter</name>
				</plotter>
				<plotter>
					<name>edu.usf.experiment.plot.SummarizeRuntimes</name>
				</plotter>
				<plotter>
					<name>edu.usf.experiment.plot.PathPlotter</name>
				</plotter>
				<plotter>
					<name>edu.usf.experiment.plot.ValuePlotter</name>
				</plotter>
			</afterEpisodePlotters>
		</episodes>
		
		
		<afterTrialTasks>
		</afterTrialTasks>
		<afterTrialLoggers>
		</afterTrialLoggers>
		<afterTrialPlotters>
		</afterTrialPlotters>
	</trial>
	
