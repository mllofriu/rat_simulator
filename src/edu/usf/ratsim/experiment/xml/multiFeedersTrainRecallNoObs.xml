	<trial>
		<name>Recall</name>
		
		<groups>
			<group>Control</group>
			<group>Dorsal</group>
			<group>Ventral</group>
			<group>NegControl</group>
			<group>TimeoutControl</group>
		</groups>
		<beforeTrialTasks>
			<task>
				<name>edu.usf.experiment.task.DeactivateHPCLayers</name>
				<params>
					<group>Dorsal</group>
					<layers>0</layers>
				</params>
			</task>
			<task>
				<name>edu.usf.experiment.task.DeactivateHPCLayers</name>
				<params>
					<group>Ventral</group>
					<layers>1</layers>
				</params>
			</task>
			<task>
				<name>edu.usf.experiment.task.DeactivateHPCLayers</name>
				<params>
					<group>NegControl</group>
					<layers>0,1</layers>
				</params>
			</task>
		</beforeTrialTasks>
		<beforeTrialLoggers>
		</beforeTrialLoggers>
		<episodes>
			<number>1</number>
			<sleep>00</sleep>
			<beforeEpisodeTasks>
				<task>
					<name>edu.usf.experiment.task.UnFlashAllFeeders</name>
