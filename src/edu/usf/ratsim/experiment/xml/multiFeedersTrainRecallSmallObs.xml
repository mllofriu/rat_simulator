					<name>edu.usf.experiment.task.FlashActivateRandomFeeder</name>
					<params>
					</params>
				</task>
				<task>
					<name>edu.usf.experiment.task.PlaceRobotInitally</name>
					<params>
						<point>
							<x>0</x>
							<y>0</y>
							<theta>3.14</theta>
						</point>
					</params>
				</task>
			</beforeEpisodeTasks>
			<beforeCycleTasks>
			</beforeCycleTasks>
			<afterCycleTasks>
