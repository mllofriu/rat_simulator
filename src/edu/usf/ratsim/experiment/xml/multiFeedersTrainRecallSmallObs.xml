	</beforeExperimentPlotters>
	
	<trial>
		<name>Training</name>
		
		<groups>
			<group>Control</group>
			<group>Dorsal</group>
			<group>Ventral</group>
			<group>NegControl</group>
		</groups>
		<beforeTrialTasks>
		</beforeTrialTasks>
		<episodes>
			<number>1</number>
			<sleep>00</sleep>
			<beforeEpisodeTasks>
				<!--<task>
					<name>edu.usf.experiment.task.ActivateAllEnabledFeeders</name>
					<params>
					</params>
				</task>-->
