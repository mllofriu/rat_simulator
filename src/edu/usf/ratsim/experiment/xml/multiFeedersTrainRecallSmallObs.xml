<?xml version="1.0" encoding="us-ascii"?>
	
<experiment>
	
	<universe>
		<name>edu.usf.ratsim.experiment.universe.virtual.VirtUniverse</name>
		<params>
			<closeToFoodThrs>0.15</closeToFoodThrs>
			<maze>src/edu/usf/ratsim/experiment/mazes/multiFeeders.xml</maze>
			<display>false</display>
		</params>
	</universe>
	
	<robot>
		<name>edu.usf.ratsim.robot.virtual.VirtualRobot</name>
		<params>
			<noise>.2</noise>
			<translationRotationNoise>.195</translationRotationNoise>
			<lookaheadSteps>5</lookaheadSteps>
			<halfFieldOfView>1.83</halfFieldOfView>
			<visionDist>10</visionDist>
			<closeThrs>.1</closeThrs>
		</params>
	</robot>
	
	<model>
		<name>edu.usf.ratsim.experiment.subject.MultiScaleArtificialPCSubject</name>
		<params>
			<step>.05</step>
			<leftAngle>0.195</leftAngle>
			<rightAngle>-0.195</rightAngle>
			<numCCLayers>3</numCCLayers>
			<numCCCellsPerLayer>100000</numCCCellsPerLayer>
			<minPCRadius>.05</minPCRadius>
			<maxPCRadius>.3</maxPCRadius>
			<placeCells>wallGoalExponential</placeCells>
			<goalCellProportion>0.3</goalCellProportion>
			<minHDRadius>.195</minHDRadius>
			<maxHDRadius>1.56</maxHDRadius>
			<numIntentions>8</numIntentions>
			<deterministicActionSelection>true</deterministicActionSelection>
			<foodReward>10000</foodReward>
			<nonFoodReward>-100</nonFoodReward>
			<taxicDiscountFactor>1</taxicDiscountFactor>
			<rlDiscountFactor>1</rlDiscountFactor>
			<alpha>.5</alpha>
			<tracesDecay>.5</tracesDecay>
			<initialValue>0</initialValue>
			<flashingReward>5000</flashingReward>
			<nonFlashingReward>500</nonFlashingReward>
			<rememberLastTwo>true</rememberLastTwo>
			<estimateValue>true</estimateValue>
			<cellContribution>1</cellContribution>
			<explorationReward>100</explorationReward>
			<attentionExploringVal>100</attentionExploringVal>
			<maxAttentionSpan>20</maxAttentionSpan>
			<explorationHalfLifeVal>1</explorationHalfLifeVal>
			<rlType>actorCritic</rlType>
			<voteType>gradient</voteType>
			<votesConnProbs>1,.5,0</votesConnProbs>
			<valueConnProbs>0,.5,1</valueConnProbs>
			<xmin>-.5</xmin>
			<ymin>-.5</ymin>
			<xmax>.5</xmax>
			<ymax>.5</ymax>
			<maxActionsSinceForward>10</maxActionsSinceForward>
			<stillExplorationVal>10000</stillExplorationVal>
		</params>
	</model>
	
	
	<group>
		<name>Control</name>
		<numMembers>8</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	 <group>
		<name>Dorsal</name>
		<numMembers>8</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	<group>
		<name>Ventral</name>
		<numMembers>8</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	<group>
		<name>NegControl</name>
		<numMembers>8</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
	
	
	<group>
		<name>TimeoutControl</name>
		<numMembers>8</numMembers>
		<params>
		</params>
		<tasks>
		</tasks>
	</group>
	
		
	<beforeExperimentTasks>
		<!--<task> <name>edu.usf.experiment.task.EnableRandomFeedersTriangle</name> <params>
			</params> </task>-->
		<task>
			<name>edu.usf.experiment.task.EnableFeeders</name>
			<params>
				<feeders>0,2,5</feeders>
			</params>
		</task>
	</beforeExperimentTasks>
	
	<beforeExperimentLoggers>
		<logger>
			<name>edu.usf.experiment.log.WallLogger</name>
			<params>
			</params>
		</logger>
