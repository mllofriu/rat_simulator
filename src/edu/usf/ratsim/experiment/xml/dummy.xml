<?xml version="1.0" encoding="us-ascii"?>
<!-- Just a test of new format -->

<experiment>
	<universe>
		<name>DummyUniverse</name>
	</universe>
	
	<subject>
		<name>RatSubject</name>
	</subject>
	
	<group>
		<name>A</name>
		<numMembers>1</numMembers>
		<params></params>
		<tasks>
		</tasks>
	</group>
	<group>
		<name>B</name>
		<numMembers>1</numMembers>
		<model>
			<name>DummyModel</name>
			<params>
				<text>Params for Group B</text>
			</params>
		</model>
		<tasks>
		</tasks>
	</group>
	
	<beforeTasks>
		<task>
			<name>DummyTask</name>
			<params>
				<text>Initial Experiment Task</text>
			</params>
		</task>
	</beforeTasks>
	
	<trial>
		<name>Training</name>
		<groups>
			<group>A</group>
			<group>B</group>
		</groups>
		<beforeTasks>
			<task>
				<name>DummyTask</name>
				<params>
					<text>Initial Trial Task</text>
				</params>
			</task>
		</beforeTasks>
		<episodes>
			<number>2</number>
			<beforeTasks>
				<task>
					<name>DummyTask</name>
					<params>
						<text>Initial Episode Task</text>
					</params>
				</task>
			</beforeTasks>
			<beforeCycleTasks>
				<task>
					<name>DummyTask</name>
					<params>
						<text>Before Cycle Task</text>
					</params>
				</task>
			</beforeCycleTasks>
			<afterCycleTasks>
				<task>
					<name>DummyTask</name>
					<params>
						<text>After Cycle Task</text>
					</params>
				</task>
			</afterCycleTasks>
			<loggers>
				<logger>
					<name>SubjectPositionLogger</name>
					<params></params>
				</logger>
			</loggers>
			<afterTasks>
				<task>
					<name>DummyTask</name>
					<params>
						<text>After Episode Task</text>
					</params>
				</task>
			</afterTasks>
			<stopConditions>
				<condition>
					<name>MaxCycles</name>
					<params>
						<max>100</max>
					</params>
				</condition>
			</stopConditions>
			<loggers>
				<logger>
					<name>DummyLogger</name>
				</logger>
			</loggers>
			<plotters>
				<plotter>
					<name>DummyPlotter</name>
				</plotter>
			</plotters>
		</episodes>
		
		<afterTasks>
			<task>
				<name>DummyTask</name>
				<params>
				</params>
			</task>
		</afterTasks>
		<loggers>
			<logger>
				<name>DummyLogger</name>
			</logger>
		</loggers>
		<plotters>
			<plotter>
				<name>DummyPlotter</name>
			</plotter>
		</plotters>
	</trial>
	
	<!-- 4 testing trials -->
	<afterTasks>
		<task>
			<name>DummyTask</name>
			<params>
				<text>After Experiment Task</text>
			</params>
		</task>
	</afterTasks>
	<loggers>
		<logger>
			<name>DummyLogger</name>
		</logger>
	</loggers>
	<plotters>
		<plotter>
			<name>DummyPlotter</name>
		</plotter>
	</plotters>
</experiment>
